@model List<ThomasSalon.Abstracciones.Modelos.Sucursales.SucursalesDto>

@if (Model != null && Model.Any())
{
    foreach (var sucursal in Model)
    {
        <a class="nav-link" href="@Url.Action("ListarAgendas", "Citas", new { idSucursal = sucursal.IdSucursal })">
            @sucursal.Nombre
        </a>
    }
}
else
{
    <span class="nav-link text-muted">No hay sucursales disponibles</span>
}
